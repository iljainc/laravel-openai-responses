{
    "name": "idpromogroup/laravel-openai-responses",
    "description": "Laravel package for standardizing OpenAI API responses",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "IDPromoGroup",
            "email": "idpromogroup@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Idpromogroup\\LaravelOpenaiResponses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Idpromogroup\\LaravelOpenaiResponses\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idpromogroup\\LaravelOpenaiResponses\\LaravelOpenaiResponsesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
